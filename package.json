{
  "name": "@agentic-profile/a2a-client",
  "version": "0.4.16",
  "description": "Typescript client for AI Agents using A2A and the Agentic Profile",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "tsup",
    "a2a:cli": "npx tsx --inspect=9230 tests/cli.ts",
    "setup-my-agent": "npx tsx scripts/create-global-agentic-profile.ts",
    "build": "tsup",
    "test": "jest"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentic-profile/a2a-client.git"
  },
  "author": "Mike Prince",
  "license": "Apache-2",
  "bugs": {
    "url": "https://github.com/agentic-profile/a2a-client/issues"
  },
  "homepage": "https://github.com/agentic-profile/a2a-client#readme",
  "dependencies": {
    "@agentic-profile/auth": "^0.4.10"
  },
  "devDependencies": {
    "@agentic-profile/express-common": "^0.4.14",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.9.1"
}
